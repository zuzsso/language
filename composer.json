{
    "name": "zuzsso/language",
    "description": "Language utils",
    "type": "library",
    "authors": [
        {
            "name": "Nico Santamaria",
            "email": "ndsans@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "config": {
        "platform": {
            "php": "7.4.28"
        }
    },
    "autoload": {
        "psr-4": {
            "Language\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Language\\Tests\\": "tests/"
        }
    },
    "require": {
        "php-di/php-di": "^6.2",
        "zuzsso/di-manifest": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.2.3",
        "phpmd/phpmd": "@stable",
        "friendsofphp/php-cs-fixer": "*"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/zuzsso/di-manifest"
        }
    ]
}
